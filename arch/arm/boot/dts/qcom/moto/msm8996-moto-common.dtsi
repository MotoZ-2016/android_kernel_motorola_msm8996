&i2c_7 {
	fusb302@22 {
		compatible = "fairchild,fusb302";
		reg = <0x22>;
		gpios = <&tlmm 38 1>, /* FUSB302_INT_N, in */
			<&tlmm 135 0>, /* USB_AUD_SW_SEL, output-low */
			<&pm8994_mpps 4 2>; /* USB_AUD_DET_N, output-high */
		fusb,gpio-labels = "gpio_fusb_int_n",
				   "gpio_usb_aud_sw_sel",
				   "gpio_usb_aud_det_n";
		pinctrl-names = "default", "active";
		pinctrl-0 = <&fusb302_int_default &fusb302_aud_sw_sel_default>;
		pinctrl-1 = <&fusb302_int_active &fusb302_aud_sw_sel_active>;
	};
	fsa8500@23 {
		compatible = "fairchild,fsa8500";
		reg = <0x23>;
		gpios = <&tlmm 63 0x0>;
		fsa8500-init-regs = <0x0c 0x41>,
				    <0x0f 0xf9>,
				    <0x11 0x84>,
				    <0x13 0xbf>,
				    <0x14 0x69>;

		fsa8500-keymap = <0x4000000 0xe2>, /* SND_JACK_BTN_0 - KEY_MEDIA */
				 <0x0400000 0x246>, /* SND_JACK_BTN_4 - KEY_VOICECOMMAND */
				 <0x0200000 0x73>, /* SND_JACK_BTN_5 - KEY_VOLUMEUP */
				 <0x0100000 0x72>; /* SND_JACK_BTN_6 - KEY_VOLUMEDOWN */

		pinctrl-names = "default";
		pinctrl-0 = <&fsa8500_hs_det_default>;
		fsa8500-lint-disabled;
	};

	cs35l34@41 {
		compatible = "cirrus,cs35l34";
		reg = <0x41>;

		cirrus,gain-zc = <1>;
		pinctrl-names = "default";
		pinctrl-0 = <&cs35l34_reset_default>;
	};

	drv2605: drv2605@5a {
		compatible = "ti,drv2605";
		reg = <0x5a>;
		gpios = <&pmi8994_gpios 9 0>;   /* EN_GPIO */
		external_trigger = <1>;         /* Enable ext. trigger */
		default_effect = <4>;           /* Sharp Click 100% */
		rated_voltage = <0x72>;         /* 2.4v */
		overdrive_voltage = <0xd1>;     /* 4.6v */
		rtp_overdrive_voltage = <0xea>; /* 4.991v */
		static-vdd-supply = <&vreg_5p0>;
	};

	pnxxx: pnxxx@28 {
		compatible = "nxp,pn544";
		reg = <0x28>;
		interrupt-parent = <&tlmm>;
		interrupts = <9 0>;
		gpios = <&tlmm 9 0>,     /* IRQ */
		    <&tlmm 28 0>,  /* VEN */
		    <&tlmm 12 0>;  /* Firmware */

		nxp,pnxxx-discharge-delay = <100>;
		nxp,pnxxx-ven-inv-polarity = <0>;


		pinctrl-names = "default";
		pinctrl-0 = <&nfc_irq_default &nfc_en_default &nfc_firm_default>;
	};

	tmp108@48 {
		compatible = "ti,tmp108";
		reg = <0x48>;
		interrupt-parent = <&tlmm>;
		gpios = <&tlmm 80 0>;

		pinctrl-names = "default";
		pinctrl-0 = <&tmp108_irq_default>;
	};

	usb3813@2D {
		compatible = "microchip,usb3813";
		reg = <0x2D>;
		gpios = <&pmi8994_gpios 5 0x0>,   /* HUB_RESET_EN */
			<&pmi8994_gpios 6 0x1>;    /* HUB_INT_EN */
		gpio-labels = "gpio_hub_reset_n",
			"gpio_hub_int_n";
		clock-names = "hub_clk";
		clocks = <&clock_gcc clk_bb_clk2>;
		enable-usbhost;
		vdd-hsic-supply = <&pm8994_l11>;
	};

	analogix_anx78e6@72 {
		compatible = "analogix,anx7816";
		status = "ok";
		reg = <0x72>;
		clock-names = "dp_bridge_mclk";
		clocks = <&clock_gcc clk_bb_clk2>;
		interrupt-parent = <&tlmm>;
		/*cbl-det-gpio is interrupt pin */
		interrupts = <14 (IRQ_TYPE_EDGE_RISING | IRQ_TYPE_EDGE_FALLING)>;
		analogix,reset-gpio = <&tlmm 93 0x00>;
/* TODO: Don't know if we need this in our configuration
		analogix,irq-gpio = <&msm_gpio 53 0x00>; */
		analogix,cbl-det-gpio = <&tlmm 14 0x00>;
		analogix,p-dwn-gpio = <&pmi8994_gpios 3 0x0>;   /* chip_pd */
		analogix,vdd_33-supply = <&pm8994_l22>;
		analogix,vdd_18-supply = <&pm8994_s4>;
		analogix,vdd_10-supply = <&pm8994_l3>;
		analogix,i2c-pull-up = <1>;
		qcom,hdmi-tx-map = <&mdss_hdmi_tx>;

		pinctrl-names = "anx_default", "anx_sleep";
		pinctrl-0 = <&anx_rst_active &anx_mydp_det_active>;
		pinctrl-1 = <&anx_rst_suspend &anx_mydp_det_suspend>;
	};

};

&soc {
	/delete-node/ qcom,wil6210;
	/delete-node/ sound-9335;
	/delete-node/ wcd9xxx-irq;
	/delete-node/ spi@7575000;

	qcom,msm-dai-mi2s {
		compatible = "qcom,msm-dai-mi2s";
		dai_mi2s: qcom,msm-dai-q6-mi2s-tert {
			compatible = "qcom,msm-dai-q6-mi2s";
			qcom,msm-dai-q6-mi2s-dev-id = <2>;
			qcom,msm-mi2s-rx-lines = <1>;
			qcom,msm-mi2s-tx-lines = <2>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&tert_mi2s_active &tert_mi2s_sd0_active &tert_mi2s_sd1_active>;
			pinctrl-1 = <&tert_mi2s_sleep &tert_mi2s_sd0_sleep &tert_mi2s_sd1_sleep>;
		};

		dai_mi2s_quat: qcom,msm-dai-q6-mi2s-quat {
			status = "disabled";
		};
	};

	ramoops {
		compatible = "ramoops";
		status = "ok";
		memory-region = <&ramoops_mem>;

		console-size = <0x0 0x00080000>;
		pmsg-size    = <0x0 0x00040000>;
		record-size  = <0x0 0x00008000>;
		ftrace-size  = <0x0 0x00020000>;
	};

	wdog: qcom,wdt@9830000 {
		qcom,bark-time = <22000>;
	};

	slim_msm: slim@91c0000 {
		/delete-node/ tomtom_codec;
		/delete-node/ tasha_codec;
	};

	wlan_en_vreg: wlan_en_vreg {
		gpio = <&tlmm 121 0>;
	};

	qcom,cnss {
		/delete-property/ wlan-bootstrap-gpio; /* Pulled high with S4 */
		qcom,wlan-rc-num = <1>;
	};

	vreg_5p0: vreg_5v {
		compatible = "regulator-fixed";
		regulator-name = "vreg_5p0";
		gpio = <&pm8994_gpios 3 0>;
		enable-active-high;
	};

	dummy_vreg: dummy_vreg {
		compatible = "regulator-fixed";
		status = "ok";
		regulator-name = "dummy_vreg";
		regulator-always-on;
	};
	/* BLSP12 not used for i2c */
	/delete-node/ i2c@75ba000;

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";

		vol_up {
			label = "volume_up";
			gpios = <&pm8994_gpios 2 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	/* SensorHub Bus */
	i2c_5: i2c@7579000 { /* BLSP1 QUP5 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x7579000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 99 0>;
		dmas = <&dma_blsp1 20 32 0x20000020 0x20>,
			<&dma_blsp1 21 32 0x20000020 0x20>;
