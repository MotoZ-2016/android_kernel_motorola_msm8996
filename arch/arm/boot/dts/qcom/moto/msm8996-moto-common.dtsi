		/delete-property/ bias-pull-down;
		bias-pull-up;
	};
};

&firmware {
	android {
		fstab {
			/delete-node/ vendor;
			system {
				dev = "/dev/block/platform/soc/624000.ufshc/by-name/system";
			};
		};
	};
};

&slim_msm {
	/delete-node/ tasha_codec;
	florida-codec {
		compatible = "wlf,florida-slim-audio";
		elemental-addr = [00 7F 10 51 2F 01];
	};
};

&ufs1 {
	qcom,pm-qos-cpu-group-latency-us = <30 30>;
};

&pcie1 {
	wake-gpio =  <&tlmm 132 0>;
};

&pmi8994_boost_5v {
	/delete-property/ parent-supply;
};

&pmi8994_boost_pin_ctrl {
	/delete-property/ parent-supply;
};
